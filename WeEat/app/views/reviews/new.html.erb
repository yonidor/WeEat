<%= form_for([@restaurant, @review]) do |f| %>
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :author_name %>
    </p>

     <p>
      <%= f.label :rating %><br>
      <%= f.text_field :rating %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :review_text %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>